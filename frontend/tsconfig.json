{
    "compilerOptions": {
        "resolveJsonModule": true,
        "strict": true,
        "baseUrl": "./",
        "paths": {
            "@assets/*": ["./src/assets/*"],
            "@components/*": ["./src/components/*"],
            "@screens/*": ["./src/screens/*"],
            "@storage/*": ["./src/storage/*"],
            "@utils/*": ["./src/utils/*"],
            "@service/*": ["./src/service/*"],
            "@routes/*": ["./src/routes/*"],
            "@interfaces/*": ["./src/interfaces/*"],
            "@locales/*": ["./src/i18n/*"]
        },
        "allowSyntheticDefaultImports": true,
        "jsx": "react"
    },
    "extends": "expo/tsconfig.base"
}
